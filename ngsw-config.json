{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/icons/**",
          "/**/*.svg"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "metadata",
      "urls": [
        "https://d3d4yli4hf5bmh.cloudfront.net/metadatav2.json"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 1,
        "maxAge": "10s",
        "timeout": "5s"
      }
    },
    {
      "name": "cover-art",
      "urls": [
        "https://d3d4yli4hf5bmh.cloudfront.net/cover.jpg"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 5,
        "maxAge": "1h",
        "timeout": "3s"
      }
    },
    {
      "name": "api",
      "urls": [
        "/api/**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 10,
        "maxAge": "1m",
        "timeout": "5s"
      }
    }
  ]
}
